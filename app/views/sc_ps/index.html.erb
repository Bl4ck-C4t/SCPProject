<p id="notice"><%= notice %></p>

<h1>Scps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scps.each do |scp| %>
      <tr>
        <td><%= link_to 'Show', scp %></td>
        <td><%= link_to 'Edit', edit_scp_path(scp) %></td>
        <td><%= link_to 'Destroy', scp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scp', new_scp_path %>
